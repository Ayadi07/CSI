	<queryString>
		<![CDATA[SELECT
     investor.`EMAIL` AS investor_EMAIL,
     investor.`LASTNAME` AS investor_LASTNAME,
     investor.`MOBILE` AS investor_MOBILE,
     investor.`NAME` AS investor_NAME,
     investor.`ACCOUNT_ID` AS investor_ACCOUNT_ID,
     portfolio.`RIRATE` AS portfolio_RIRATE,
     portfolio.`INITIALMONEY` AS portfolio_INITIALMONEY,
     portfolio.`INVESTEDMONEY` AS portfolio_INVESTEDMONEY,
     portfolio.`LIQUIDMONEY` AS portfolio_LIQUIDMONEY,
     portfolio.`NAME` AS portfolio_NAME,
     portfolio.`PORTFOLIOPERFORMANCE` AS portfolio_PORTFOLIOPERFORMANCE,
     portfolio.`PORTFOLIORETURN` AS portfolio_PORTFOLIORETURN,
     portfolio.`PORTFOLIOTODAYVALUE` AS portfolio_PORTFOLIOTODAYVALUE,
     portfolio.`POTENTIALEARNINGS` AS portfolio_POTENTIALEARNINGS,
     consultant.`NAME` AS consultant_NAME,
     consultant.`EMAIL` AS consultant_EMAIL,
     market.`MONTHLYVARIANCE` AS market_MONTHLYVARIANCE,
     market.`MARKETID` AS market_MARKETID,
     market.`INDICE` AS market_INDICE,
     account.`INITIALMONEY` AS account_INITIALMONEY,
     account.`INVESTEDMONEY` AS account_INVESTEDMONEY,
     account.`MONEY` AS account_MONEY,
     portfolio.`PORTFOLIOBETA` AS portfolio_PORTFOLIOBETA
FROM
     `portfolio` portfolio INNER JOIN `investor` investor ON portfolio.`ACCOUNT_ID` = investor.`ACCOUNT_ID`
     INNER JOIN `consultant` consultant ON investor.`CONSULTANT_NAME` = consultant.`NAME`
     INNER JOIN `account` account ON investor.`ACCOUNT_ID` = account.`ID`
     AND account.`ID` = portfolio.`ACCOUNT_ID`
     INNER JOIN `market` market ON portfolio.`MARKET_MARKETID` = market.`MARKETID`
where investor.`ACCOUNT_ID`= 201]]>
	</queryString>
